{
  "description": "Safely connect your local wallet with a website using Dero RPC Bridge.",
  "manifest_version": 2,
  "name": "Dero RPC Bridge",
  "version": "1.5.2",
  "homepage_url": "https://github.com/g45t345rt/dero-rpc-bridge/tree/master/extension",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}